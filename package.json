{
  "name": "karma-typescript-mocha-chai-sinon-es6-modules",
  "version": "0.1.0",
  "description": "A boilerplate dev environment for typescript, mocha, chai, and sinon for karma unit testing (tdd/bdd)",
  "main": "",
  "scripts": {
    "test": "karma start --single-run --browsers ChromeHeadless,FirefoxHeadless karma.conf.js",
    "test:chrome": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test:chromeHeaded": "karma start --browsers Chrome karma.conf.js",
    "test:firefox": "karma start --single-run --browsers FirefoxHeadless karma.conf.js",
    "test:edge": "karma start --single-run --browsers Edge karma.conf.js",
    "test:ie": "karma start --browsers IE karma.ie.conf.js",
    "test:safari": "karma start --single-run --browsers Safari karma.conf.js",
    "test:watch": "karma watch --browsers Chrome karma.conf.js"
  },
  "author": "AaronNGray",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "chai": "^4.0.2",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.3.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sinon": "^1.0.5",
    "karma-sonarqube-reporter": "^1.3.0",
    "karma-sonarqube-unit-reporter": "0.0.21",
    "karma-summary-reporter": "^1.7.2",
    "karma-typescript": "^4.1.1",
    "karma-typescript-es6-transform": "^4.1.1",
    "karma-typescript-preprocessor": "^0.3.1",
    "mocha": "^7.0.0",
    "sinon": "^8.0.4",
    "standard": "^10.0.2",
    "typescript": "^2.4.1"
  }
}
